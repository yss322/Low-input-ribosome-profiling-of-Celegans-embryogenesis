# Step 1: Run compareCluster with Roman numeral labels
wt_enrichment_results <- compareCluster(
geneClusters = wt_gene_lists,
fun = "enrichGO",
OrgDb = org.Ce.eg.db,
keyType = "SYMBOL",
ont = "ALL",
pAdjustMethod = "BH",
pvalueCutoff = 0.2,
qvalueCutoff = 0.2,
universe = background_genes
)
